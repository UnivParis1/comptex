{
  "name": "compte-externe",
  "private": true,
  "version": "0.0.0",
  "description": "Application to create users external users with modular moderation workflow",
  "repository": "https://github.com/prigaux/compte-externe",
  "license": "Apache-2.0",
  "scripts": {
    "postinstall": "cd app && npm install",
    "prestart": "npm install",
    "build": "concurrently 'tsc -p server' 'cd app && npm run build'",
    "build:w": "concurrently 'tsc --watch -p server' 'cd app && npm run build:w'",
    "dev": "concurrently 'tsc --watch -p server' 'cd app && npm run dev'",
    "start:w": "concurrently 'tsc --watch -p server/' 'cd app && npm run build:w' 'NODE_ENV=test nodemon --watch server/ start-server.js'",
    "test:w": "mocha --require source-map-support/register --recursive --watch server/tests"
  },
  "engines": {
    "node": "^6.4"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/concat-stream": "^1.6.0",
    "@types/csvtojson": "^1.1.5",
    "@types/diff": "^3.2.2",
    "@types/es6-promise": "0.0.32",
    "@types/express": "^4.0.36",
    "@types/jasmine": "^2.5.53",
    "@types/lodash": "^4.14.x",
    "@types/mocha": "^2.2.41",
    "@types/mongodb": "^2.2.7",
    "@types/morgan": "^1.7.32",
    "@types/mustache": "^0.8.29",
    "@types/node": "^4",
    "@types/nodemailer": "^3.1.0",
    "@types/xml2js": "^0.4.0",
    "body-parser": "~1.12.4",
    "concat-stream": "^1.5.0",
    "csvtojson": "^1.1.5",
    "debug": "~2.2.0",
    "diff": "^3.3.0",
    "es6-promise": "^4.1.1",
    "express": "~4.12.4",
    "html5-boilerplate": "0.0.1",
    "iconv-lite": "^0.4.19",
    "ldap-filter": "^0.3.3",
    "ldapjs": "^1.0.0",
    "lodash": "^4.14.x",
    "mongodb": "^2.0.33",
    "morgan": "~1.5.3",
    "mustache": "^2.1.1",
    "nodemailer": "^1.3.4",
    "nodemailer-sendmail-transport": "^1.0.0",
    "promise-polyfill": "^6.0.2",
    "promise.prototype.finally": "^3.1.0",
    "simple-get": "^1.3.3",
    "typescript": "2.6.x",
    "xml2js": "^0.4.9"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "eslint": "^4.13.1",
    "eslint-plugin-vue": "^4.0.0-beta.2",
    "live-reload": "^1.1.0",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "phantomjs": "^1.9.17",
    "source-map-support": "^0.3.3"
  }
}
