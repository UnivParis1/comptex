{
  "name": "comptex",
  "version": "1.0.0",
  "description": "Application to create users external users with modular moderation workflow",
  "author": "Pascal Rigaux <pascal.rigaux@univ-paris1.fr>",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:atomic": "vite build --outDir dist.new && rm -rf dist && mv dist.new dist",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^6.x",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-fr": "^3.0.2",
    "axios": "^1.13.2",
    "bootstrap": "^3.3.7",
    "croppie": "^2.6.3",
    "diff": "^8.0.2",
    "exif-js": "^2.3.0",
    "fflate": "^0.8.2",
    "load-script-once": "^2.0.1",
    "prismjs": "^1.24.1",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.1.4"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.4.6",
    "axios-mock-adapter": "^2.1.0",
    "jsdom": "^27.2.0",
    "vitest": "^4.0.13",
    "vue-template-compiler": "^2.7.10"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
